{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "app/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/predict",
      "dest": "/api/index.py"
    },
    {
      "src": "/",
      "dest": "/app/static/index.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/app/static/$1"
    }
  ]
}