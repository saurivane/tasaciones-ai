{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/predict",
      "destination": "/api/index.py"
    },
    {
      "source": "/static/(.*)",
      "destination": "/app/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/app/static/index.html"
    }
  ]
}